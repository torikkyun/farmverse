{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2023",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@app/*": [
        "apps/back-end/src/*"
      ],
      "@queue/*": [
        "apps/queue/src/*"
      ],
      "@prisma/*": [
        "generated/prisma/*"
      ],
      "@common/*": [
        "apps/back-end/src/common/*"
      ],
      "@models/*": [
        "apps/back-end/src/models/*"
      ],
      "@dto/*": [
        "apps/back-end/src/common/dto/*"
      ],
      "@providers/*": [
        "apps/back-end/src/providers/*"
      ],
      "@static/*": [
        "static/*"
      ]
    }
  }
}